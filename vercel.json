{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOWALL"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors *"
        }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/focus-cron",
      "schedule": "* * * * *"
    },
    {
      "path": "/api/break-cron",
      "schedule": "* * * * *"
    }
  ]
}
